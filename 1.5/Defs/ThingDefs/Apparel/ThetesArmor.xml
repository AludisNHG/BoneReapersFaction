<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="ArmorSmithableBase">
    <defName>Apparel_ThetesCuirass</defName>
    <label>Thetes Cuirass</label>
    <description>Overlapping solid plates of armor covering the entire body from neck to feet.</description>
    <recipeMaker Inherit="False">
	  <recipeUsers>
        <li>pphhyy_YoroiBench</li>
      </recipeUsers>
      <unfinishedThingDef>UnfinishedMetallicTechArmor</unfinishedThingDef>
      <researchPrerequisite>pphhyy_BasicArmorTech</researchPrerequisite>
      <skillRequirements>
        <Crafting>7</Crafting>
      </skillRequirements>
      <displayPriority>250</displayPriority>
    </recipeMaker>
    <graphicData>
      <texPath>Armor/ThetesCuirass/Thetes Cuirass</texPath>
      <graphicClass>Graphic_Single</graphicClass>
	  <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
    <costStuffCount>160</costStuffCount>
    <statBases>
      <WorkToMake>38000</WorkToMake>
      <MaxHitPoints>290</MaxHitPoints>
      <Mass>15</Mass>
	  <Flammability>0</Flammability>
      <StuffEffectMultiplierArmor>0.73</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>1.0</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>15</EquipDelay>
    </statBases>
    <thingSetMakerTags>
      <li>RewardStandardQualitySuper</li>
    </thingSetMakerTags>
    <thingCategories>
      <li>ApparelArmor</li>
    </thingCategories>
    <costList>
      <pphhyy_HumanBones>30</pphhyy_HumanBones>
    </costList>
    <tradeTags>
      <li>Clothing</li>
      <li>Armor</li>
    </tradeTags>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
        <li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>Armor/ThetesCuirass/Thetes Cuirass</wornGraphicPath>
      <layers>
	    <li>Middle</li>
        <li>Shell</li>
      </layers>
      <tags>
        <li>MedievalMilitary</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
	  <useWornGraphicMask>true</useWornGraphicMask>
    </apparel>
  </ThingDef>
  
