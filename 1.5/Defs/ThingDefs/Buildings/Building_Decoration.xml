<!-- Osseous Brazier -->

<ThingDef ParentName="BuildingBase">
  <defName>pphhyy_OsseousBrazier</defName>
  <label>osseous brazier</label>
  <description>PLACEHOLDER</description>
  <altitudeLayer>Building</altitudeLayer>
  <passability>PassThroughOnly</passability>
  <selectable>true</selectable>
  <fillPercent>0.20</fillPercent>
  <pathCost>14</pathCost>
  <tickerType>Normal</tickerType>
  <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
  <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
  <designationCategory>Furniture</designationCategory>
  <rotatable>false</rotatable>
  <size>(3,3)</size>
  <graphicData>
    <graphicClass>Graphic_Single</graphicClass>
    <texPath>Buildings/OsseousBrazier/OsseousBrazier_south</texPath>
    <shaderType>CutoutComplex</shaderType>
    <drawSize>(4.2,3.3)</drawSize>
  </graphicData>
  <statBases>
    <MaxHitPoints>100</MaxHitPoints>
    <Flammability>0.5</Flammability>
  </statBases>
  <costStuffCount>20</costStuffCount>
  <stuffCategories>
    <li>Metallic</li>
    <li>Stony</li>
    <li>Woody</li>
  </stuffCategories>
  <costList>
    <pphhyy_HumanBones>5</pphhyy_HumanBones>
  </costList>
  <comps>
    <li Class="CompProperties_Refuelable">
      <fuelConsumptionRate>2.0</fuelConsumptionRate>
      <fuelCapacity>30.0</fuelCapacity>
      <fuelConsumptionPerTickInRain>0.0006</fuelConsumptionPerTickInRain>
      <fuelFilter>
        <thingDefs>
          <li>WoodLog</li>
        </thingDefs>
      </fuelFilter>
      <initialFuelPercent>0</initialFuelPercent>
      <showAllowAutoRefuelToggle>true</showAllowAutoRefuelToggle>
    </li>
    <li Class="CompProperties_Glower">
      <glowRadius>10</glowRadius>
      <glowColor>(252,187,113,0)</glowColor>
    </li>
    <li Class="CompProperties_HeatPusher">
      <compClass>CompHeatPusherPowered</compClass>
      <heatPerSecond>3.5</heatPerSecond>
      <heatPushMaxTemperature>23</heatPushMaxTemperature>
    </li>
    <li Class="CompProperties_FireOverlay">
      <fireSize>1</fireSize>
      <offset>(0,0,0.18)</offset>
    </li>
    </comps>
    <researchPrerequisites>
      <li>pphhyy_BoneReaperBasicPillars</li>
    </researchPrerequisites>
</ThingDef>

