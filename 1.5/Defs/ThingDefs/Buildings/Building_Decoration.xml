<Defs>

  <!-- Osseous pillar -->

  <ThingDef ParentName="BuildingBase">
    <defName>pphhyy_OsseousPillar</defName>
    <label>osseous pillar</label>
    <description>PLACEHOLDER</description>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <selectable>true</selectable>
    <fillPercent>0.20</fillPercent>
    <pathCost>14</pathCost>
    <soundImpactDefault>BulletImpact_Metal</soundImpactDefault>
    <designationCategory>Furniture</designationCategory>
    <rotatable>false</rotatable>
    <size>(3,3)</size>
    <graphicData>
      <graphicClass>Graphic_Single</graphicClass>
      <texPath>Buildings/OsseousPillar/OsseousPillar_south</texPath>
      <shaderType>CutoutComplex</shaderType>
      <drawSize>(4.6,3.6)</drawSize>
    </graphicData>
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <Flammability>0.5</Flammability>
    </statBases>
    <costStuffCount>150</costStuffCount>
    <stuffCategories>
      <li>Metallic</li>
      <li>Stony</li>
      <li>Woody</li>
    </stuffCategories>
    <costList>
      <pphhyy_HumanBones>20</pphhyy_HumanBones>
    </costList>
    <researchPrerequisites>
      <li>pphhyy_BoneReaperGrandPillars</li>
